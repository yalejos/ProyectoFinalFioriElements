{
  "name": "legal_guardians-ui",
  "version": "0.0.1",
  "description": "Build and deployment scripts",
  "scripts": {
    "clean": "rimraf --glob resources mta_archives mta-op*",
    "build": "rimraf resources mta_archives && mbt build --mtar archive",
    "deploy": "cf deploy mta_archives/archive.mtar --retries 1",
    "undeploy": "cf undeploy legal_guardians-ui --delete-services --delete-service-keys --delete-service-brokers"
  },
  "devDependencies": {
    "@ui5/cli": "^4.0.0",
    "mbt": "^1.2.24",
    "rimraf": "^6.0.1"
  },
  "dependencies": {
    "@sapui5/ts-types-esm": "^1.145.0"
  }
}
